<h1>Welcome to <%=@artist.name%> Page</h1>

<h5><%= link_to "Return to Index Page", artists_path %></h5>
<h5><%= link_to "Edit aritst", edit_artist_path(@artist) %></h5>
<h5><%= button_to "Delete aritst", artist_path(@artist), method: :delete %></h5>

<h3>Artist Name: <%=@artist.name%></h3><br>
<h3>Artist Bio: <%=@artist.bio%> </h3><br>
<%@artist.songs.each do |song|%>
    <p><h5> <%= link_to song.name, song%> </h5></p>
<% end %>

