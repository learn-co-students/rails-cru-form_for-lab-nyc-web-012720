<%= form_for @song do |f| %>
<%= f.label :name, "name" %>
<%= f.text_area :name %>
<%= f.label :artist_id, "artist" %>
<%= f.collection_select(:artist_id, Artist.all, :id, :name) %>
<%= f.text_area :artist_id %>
<%= f.label :genre_id, "genre" %>
<%= f.collection_select(:genre_id, Genre.all, :id, :name) %>
<%= f.text_area :genre_id %>
<%= f.submit %>
<% end %>